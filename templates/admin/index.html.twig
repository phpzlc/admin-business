            <el-table-column prop="PHPZlc" label="PHPZlc" >
            </el-table-column>

            <el-table-column  prop="date" label="操作" width="450">
                <template slot-scope="scope">
                    <el-button size="mini" @click="handleEdit(scope.$index, scope.row)">知了虫</el-button>
                </template>
            </el-table-column>

        </el-table>

